{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "AG News mini (HDBSCAN)",
      "type": "python",
      "request": "launch",
      "module": "alien_taxonomy.alien_taxonomy",
      "args": [
        "--domain", "text",
        "--models", "sentence-transformers/all-mpnet-base-v2", "intfloat/e5-base-v2",
        "--clusterer", "hdbscan",
        "--limit", "200",
        "--output_dir", "runs/agnews_mini"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "CIFAR mini (k-means)",
      "type": "python",
      "request": "launch",
      "module": "alien_taxonomy.alien_taxonomy",
      "args": [
        "--domain", "image",
        "--models", "openai/clip-vit-base-patch32", "google/vit-base-patch16-224",
        "--clusterer", "kmeans",
        "--limit", "500",
        "--output_dir", "runs/cifar_mini"
      ],
      "console": "integratedTerminal"
    }
  ]
}
